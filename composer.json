{
    "name": "temp/fn",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Wentz",
            "email": "stephan@wentz.it"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "brainbits/phpcs-standard": "^8",
        "brainbits/phpstan-rules": "^4",
        "phpunit/phpunit": "^11",
        "jangregor/phpstan-prophecy": "^2",
        "phpstan/phpstan-phpunit": "^2",
        "symfony/var-dumper": "^7"
    },
    "autoload": {
        "psr-4": {
            "Fnc\\": "src/"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "FncTests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
